<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.android.mealplanner">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Meal Planner"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.beldin0.android.mealplanner.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.beldin0.android.mealplanner.ActivityIngredients"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityAddIngredient"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityMeals"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityGenerate"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityShoppingList"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityAddMeal"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivitySettings"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityMealSelection"/>
        <activity android:name="com.beldin0.android.mealplanner.ActivityViewPlans">
        </activity>
    </application>

</manifest>